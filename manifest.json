{
  "manifest_version": 2,
  "name": "Cookie Vanisher",
  "version": "1.0",
  "description": "Clear cookies for a set list of domains",
  "permissions": [
    "cookies",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}